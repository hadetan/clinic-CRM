# Public (client) variables
NEXT_PUBLIC_SUPABASE_URL=https://[url].supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=AnonKey

# Server-only secret
SUPABASE_SERVICE_ROLE_KEY=ServiceRoleKey

# Use Supavisor session pooler (port 5432) for Prisma and app runtime
# Docs: https://supabase.com/docs/guides/database/prisma#add-your-connection-information-to-your-env-file
DATABASE_URL=postgresql://postgres.[projectId]:[password]@[region].pooler.supabase.com:5432/postgres?sslmode=require

DATABASE_DIRECT_URL=postgresql://postgres.[projectId]:[password]@[region].pooler.supabase.com:5432/postgres?sslmode=require